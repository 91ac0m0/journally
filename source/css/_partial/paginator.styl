.page-navigator {
	list-style: none;
	margin: 120px 0;
	padding: 0;
	text-align: center;
}

.page-navigator li,
.page-navigator span,
.page-navigator a {
	display: inline-block;
	margin: 0 4px;
}

.page-navigator a {
	display: inline-block;
	padding: 0 10px;
	height: 30px;
	line-height: 30px;
}

.page-navigator a:hover {
	background: #EEE;
	text-decoration: none;
}

.page-navigator .current a {
	color: #444;
}

/* page navigator - 选页器 */
.page-navigator li,
.page-navigator span,
.page-navigator a {
	display: inline-block;
	width: 20px;
	height: 20px;
	padding: 0;
	text-align: center;
	line-height: 20px;
	/* background: #fff; */
