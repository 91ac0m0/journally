}

.page-navigator li a,

.page-navigator a {
    padding: 3px 3px;
    border: 1px solid #314659;
    border-radius: 2px;
    margin: 0 3px;
    text-decoration: none;
    color: inherit;
}

.page-navigator li:hover {
	background: #eee;
}

.page-navigator li:hover a {
	background: #0000;
}

